# Enable the rewrite engine
RewriteEngine On

# Allow direct access to /bla/bla/ and any subdirectories
RewriteCond %{REQUEST_URI} ^/client/login [NC]

# Example: Redirect all other requests to index.php (common for front controllers)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.html [L]